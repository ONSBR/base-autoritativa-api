{
  "name": "base-autoritativa-api",
  "version": "0.0.0",
  "description": "API for several authoritative databases subjects",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src;mochista test/**/*.js test/*.js --compilers js:babel-register",
    "tdd": "npm run test -- --watch"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coichedid/base-autoritativa-api.git"
  },
  "author": "Clovis Chedid",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "base-autoritativa-connectors": ">=0.5.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "morgan": "^1.8.0",
    "node-rest-client": "^3.1.0",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.1.1",
    "mocha": "^3.3.0",
    "mochista": "^0.11.0",
    "mockery": "^2.0.0",
    "nock": "^9.0.13",
    "nodemon": "^1.9.2",
    "sinon": "^2.2.0",
    "sinon-test": "^1.0.2"
  },
  "bugs": {
    "url": "https://github.com/coichedid/base-autoritativa-api/issues"
  },
  "homepage": "https://github.com/coichedid/base-autoritativa-api#readme",
  "keywords": [
    "mapa",
    "informacao",
    "informacao",
    "graph",
    "api",
    "webapi"
  ]
}
